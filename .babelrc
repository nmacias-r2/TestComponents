{
    "presets": ["@babel/preset-typescript"],
    "plugins": [
        [
            "module:babel-loader",
            {
                "test": "\\.tsx?$",
                "exclude": "/node_modules/",
                "options": {
                    "presets": ["@babel/preset-env", "@babel/preset-react"]
                }
            }
        ]
    ]
}
